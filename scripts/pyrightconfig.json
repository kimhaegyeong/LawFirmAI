{
  "venvPath": ".",
  "venv": "venv",
  "pythonVersion": "3.11",
  "include": [
    "."
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/htmlcov",
    "**/coverage",
    "**/.pytest_cache"
  ],
  "extraPaths": [
    ".",
    "../lawfirm_langgraph",
    "../api"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}

